{
  "version": "3",
  "templates": [
    {
      "id": 1,
      "type": 1,
      "name": "cert-manager",
      "title": "Cert-Manager",
      "description": "Automatically manages and issues TLS certificates using Let's Encrypt.",
      "logo": "https://cert-manager.io/images/cert-manager-logo.svg",
      "image": "quay.io/jetstack/cert-manager-controller:v1.14.2",
      "volumes": [
          {
              "container": "/etc/letsencrypt",
              "bind": "~/.docker/.data/cert-manager/letsencrypt"
          }
      ],
      "env": [
          {
              "name": "TZ",
              "label": "Timezone",
              "default": "Europe/Athens",
              "preset": true
          }
      ],
      "labels": [
          {
              "name": "cert-manager.io/issuer",
              "value": "letsencrypt-prod"
          }
      ],
      "restart_policy": "always",
      "networks": [
          "traefik_proxy"
      ],
      "categories": [
          "Networking",
          "Security",
          "SSL"
      ],
      "maintainer": "https://github.com/hoczeus"
    },
    {
      "id": 2,
      "type": 1,
      "name": "traefik",
      "title": "Traefik v3",
      "description": "Reverse Proxy with automatic SSL, Docker service discovery, and security plugins.",
      "logo": "https://doc.traefik.io/traefik/assets/images/logo-traefik.svg",
      "image": "traefik:latest",
      "ports": [
          "80:80",
          "443:443",
          "8080:8080"
      ],
      "volumes": [
          {
              "container": "/var/run/docker.sock",
              "bind": "/var/run/docker.sock"
          },
          {
              "container": "/etc/traefik/traefik.yml",
              "bind": "~/.docker/.data/traefik/traefik.yml"
          },
          {
              "container": "/letsencrypt/acme.json",
              "bind": "~/.docker/.data/traefik/acme.json"
          },
          {
              "container": "/plugins-storage",
              "bind": "~/.docker/.data/traefik/plugins-storage"
          }
      ],
      "env": [
          {
              "name": "TZ",
              "label": "Timezone",
              "default": "Europe/Athens",
              "preset": true
          }
      ],
      "labels": [
          {
              "name": "traefik.enable",
              "value": "true"
          },
          {
              "name": "traefik.http.routers.api.rule",
              "value": "Host(`traefik.chartavellas.com`)"
          },
          {
              "name": "traefik.http.routers.api.service",
              "value": "api@internal"
          },
          {
              "name": "traefik.http.routers.api.entrypoints",
              "value": "websecure"
          },
          {
              "name": "traefik.http.routers.api.tls.certresolver",
              "value": "letsencrypt-prod"
          },
          {
              "name": "traefik.http.middlewares.auth.basicauth.users",
              "value": "admin:$apr1$randomhash$Pq98dXhXJ13QzK.5z0q5O1"
          }
      ],
      "restart_policy": "always",
      "networks": [
          "traefik_proxy"
      ],
      "categories": [
          "Networking",
          "Reverse Proxy",
          "Security"
      ],
      "maintainer": "https://github.com/hoczeus"
    },
    {
      "id": 3,
      "type": 1,
      "name": "n8n",
      "title": "n8n",
      "description": "n8n allows you to build flexible workflows focused on deep data integration.",
      "logo": "https://n8n.io/n8n-logo.png",
      "image": "docker.n8n.io/n8nio/n8n:latest",
      "ports": [
        "5678/tcp"
      ],
      "volumes": [
        {
          "container": "/home/node/.n8n",
          "bind": "~/.docker/.data/.data/n8n"
        },
        {
          "container": "/etc/localtime",
          "bind": "/etc/localtime",
          "readonly": true
        }
      ],
      "env": [
        {
          "name": "GENERIC_TIMEZONE",
          "label": "GENERIC_TIMEZONE",
          "default": "Europe/Athens",
          "preset": true
        },
        {
          "name": "TZ",
          "label": "TZ",
          "default": "Europe/Athens",
          "preset": true
        }
      ],
      "labels": [
        {
          "name": "traefik.enable",
          "value": "true"
        },
        {
          "name": "traefik.http.routers.n8n.rule",
          "value": "Host(`n8n.chartavellas.com`)"
        },
        {
          "name": "traefik.http.services.n8n.loadbalancer.server.scheme",
          "value": "http"
        },
        {
          "name": "traefik.http.services.n8n.loadbalancer.server.port",
          "value": "5678"
        },
        {
          "name": "traefik.http.routers.n8n.entrypoints",
          "value": "websecure"
        },
        {
          "name": "traefik.http.routers.n8n.tls.certresolver",
          "value": "letsencrypt-prod"
        },
        {
          "name": "traefik.http.routers.n8n.middlewares",
          "value": "basic-auth"
        }
      ],
      "restart_policy": "always",
      "networks": [
        "traefik_proxy"
      ],
      "categories": [
        "Networkother",
        "Tools"
      ],
      "maintainer": "https://github.com/hoczeus"
    }
  ]
}